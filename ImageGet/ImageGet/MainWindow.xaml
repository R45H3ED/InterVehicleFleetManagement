<Window x:Class="ImageGet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Image x:Name="image" Margin="0,-2,0,0" RenderTransformOrigin="0.5,0.5" Height="262" VerticalAlignment="Top" HorizontalAlignment="Left" Width="349">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.473"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-1.308"/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <ComboBox x:Name="combo1" HorizontalAlignment="Left" Margin="387,10,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="combo1_SelectionChanged">
            <Button/>
            <ListBoxItem Content="10:10 PM 4/11/2013"/>
            <Button/>
        </ComboBox>
    </Grid>
</Window>
